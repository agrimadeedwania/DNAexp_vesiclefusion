                <ci> ssDNA_comp1 </ci>
                <ci> ssDNA_comp2 </ci>
                <ci> ssDNA_e </ci>
              </apply>
              <apply>
                <times/>
                <ci> kre </ci>
                <ci> dsDNA </ci>
              </apply>
            </apply>
