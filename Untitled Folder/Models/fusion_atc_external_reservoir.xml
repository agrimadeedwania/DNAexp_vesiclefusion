          <unit kind="dimensionless" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="external_reservoir" name="external_reservoir" spatialDimensions="3" size="1e-06" units="litre" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="atc_e" name="atc_e" compartment="external_reservoir" initialConcentration="10000" substanceUnits="substance" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ssDNA_comp1" name="ssDNA_comp1" compartment="external_reservoir" initialConcentration="0" substanceUnits="substance" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ssDNA_comp2" name="ssDNA_comp2" compartment="external_reservoir" initialConcentration="0" substanceUnits="substance" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="ssDNA_e" name="ssDNA_e" compartment="external_reservoir" initialConcentration="0" substanceUnits="substance" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species id="dsDNA" name="dsDNA" compartment="external_reservoir" initialConcentration="0" substanceUnits="substance" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
